{
  "name": "@system-dynamics/core",
  "version": "1.3.5",
  "description": "System dynamics data model and utilities",
  "repository": "http://github.com/bpowers/model-app",
  "author": {
    "name": "Bobby Powers",
    "email": "bobbypowers@gmail.com"
  },
  "license": "Apache-2.0",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./pb": "./lib/pb/index.js",
    "./pb/*": "./lib/pb/*.js",
    "./common": "./lib/common.js",
    "./canonicalize": "./lib/canonicalize.js",
    "./datamodel": "./lib/datamodel.js",
    "./*": "./lib/*.js"
  },
  "sideEffects": false,
  "dependencies": {
    "immutable": "^5.0.3",
    "js-base64": "^3.7.7",
    "google-protobuf": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "needs-format": "prettier -l '*.ts' 'pb/*.ts'",
    "format": "prettier --write '*.ts' 'pb/*.ts'",
    "lint": "eslint .",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf ./lib",
    "build": "tsc && cp pb/*.js lib/pb/ && echo '{\"type\":\"commonjs\"}' > lib/pb/package.json"
  }
}
