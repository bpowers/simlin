{
  "name": "@simlin/core",
  "version": "1.3.5",
  "description": "System dynamics data model and utilities",
  "repository": "https://github.com/bpowers/simlin",
  "author": {
    "name": "Bobby Powers",
    "email": "bobbypowers@gmail.com"
  },
  "license": "Apache-2.0",
  "module": "lib.module",
  "main": "lib",
  "exports": {
    "./*": {
      "module": "./lib.module/*.js",
      "node": "./lib/*.js"
    },
    "./common": {
      "module": "./lib.module/common.js",
      "node": "./lib/common.js"
    },
    "./canonicalize": {
      "module": "./lib.module/canonicalize.js",
      "node": "./lib/canonicalize.js"
    },
    "./datamodel": {
      "module": "./lib.module/datamodel.js",
      "node": "./lib/datamodel.js"
    },
    ".": {
      "module": "./lib.module/index.js",
      "node": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@simlin/engine": "workspace:*",
    "immutable": "^5.0.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "needs-format": "prettier -l '*.ts'",
    "format": "prettier --write '*.ts'",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf ./lib ./lib.module",
    "build": "tsc && tsc -p tsconfig.browser.json",
    "test": "jest",
    "test:watch": "jest --watch"
  }
}
