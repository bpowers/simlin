{
  "name": "@system-dynamics/diagram",
  "version": "1.3.1",
  "description": "System Dynamics stock and flow diagram display (and editing)",
  "repository": "http://github.com/bpowers/model-app",
  "author": "Bobby Powers <bobbypowers@gmail.com>",
  "license": "Apache-2.0",
  "sideEffects": false,
  "browser": "lib.browser",
  "main": "lib",
  "exports": {
    "./*": {
      "browser": "./lib.browser/*.js",
      "node": "./lib/*.js"
    },
    ".": {
      "browser": "./lib.browser/index.js",
      "node": "./lib/index.js"
    }
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0",
    "@system-dynamics/core": "^1.3.5",
    "@system-dynamics/engine": "^1.3.1",
    "@system-dynamics/engine2": "^2.0.0",
    "chroma-js": "^3.1.2",
    "google-protobuf": "^4.0.0",
    "immutable": "^5.0.3",
    "js-base64": "^3.7.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "katex": "^0.16.0",
    "slate": "^0.112.0",
    "slate-dom": "^0.112.2",
    "slate-history": "^0.110.3",
    "slate-react": "^0.112.1",
    "wouter": "^3.6.0"
  },
  "resolutions": {
    "@types/slate*/**/immutable": "5.0.3"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.0.0",
    "@types/chroma-js": "^2.4.4",
    "@types/katex": "^0.11.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "recharts": "^3.0.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "needs-format": "prettier -l '**/*.ts' '**/*.tsx'",
    "format": "prettier --write '**/*.ts' '**/*.tsx'",
    "lint": "eslint .",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf ./lib ./lib.browser",
    "build": "tsc && tsc -p tsconfig.browser.json"
  }
}
