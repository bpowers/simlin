{
  "name": "@simlin/diagram",
  "version": "1.3.1",
  "description": "System Dynamics stock and flow diagram display (and editing)",
  "repository": "https://github.com/bpowers/simlin",
  "author": "Bobby Powers <bobbypowers@gmail.com>",
  "license": "Apache-2.0",
  "sideEffects": [
    "*.css",
    "**/*.css"
  ],
  "browser": "lib.browser",
  "main": "lib",
  "exports": {
    "./*": {
      "browser": "./lib.browser/*.js",
      "node": "./lib/*.js"
    },
    ".": {
      "browser": "./lib.browser/index.js",
      "node": "./lib/index.js"
    }
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@simlin/core": "workspace:*",
    "@simlin/engine": "workspace:*",
    "clsx": "^2.1.1",
    "downshift": "^9.2.0",
    "immutable": "^5.1.4",
    "js-base64": "^3.7.8",
    "katex": "^0.16.28",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "slate": "^0.123.0",
    "slate-dom": "^0.123.0",
    "slate-history": "^0.113.1",
    "slate-react": "^0.123.0",
    "wouter": "^3.9.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/katex": "^0.16.8",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@types/resize-observer-browser": "^0.1.11",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "needs-format": "prettier -l '**/*.ts' '**/*.tsx'",
    "format": "prettier --write '**/*.ts' '**/*.tsx'",
    "lint": "eslint .",
    "test": "jest",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf ./lib ./lib.browser",
    "build": "tsc && tsc -p tsconfig.browser.json && pnpm run build:css-stubs && pnpm run build:copy-css",
    "build:css-stubs": "find . -name '*.module.css' -not -path './lib*' -not -path './node_modules/*' | while read src; do dir=$(dirname \"$src\" | sed 's|^\\./||'); mkdir -p \"lib/$dir\"; echo \"module.exports = new Proxy({}, { get: () => '' });\" > \"lib/$dir/$(basename \"$src\")\"; done",
    "build:copy-css": "find . -name '*.css' -not -path './lib*' -not -path './node_modules/*' | while read src; do dir=$(dirname \"$src\" | sed 's|^\\./||'); mkdir -p \"lib.browser/$dir\"; cp \"$src\" \"lib.browser/$dir/\"; done"
  }
}
