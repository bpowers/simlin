{
  "name": "@system-dynamics/diagram",
  "version": "1.3.1",
  "description": "System Dynamics stock and flow diagram display (and editing)",
  "repository": "http://github.com/bpowers/model-app",
  "author": "Bobby Powers <bobbypowers@gmail.com>",
  "license": "Apache-2.0",
  "sideEffects": [
    "*.css",
    "**/*.css"
  ],
  "browser": "lib.browser",
  "main": "lib",
  "exports": {
    "./*": {
      "browser": "./lib.browser/*.js",
      "node": "./lib/*.js"
    },
    ".": {
      "browser": "./lib.browser/index.js",
      "node": "./lib/index.js"
    }
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@system-dynamics/core": "^1.3.5",
    "@system-dynamics/engine2": "^2.0.0",
    "clsx": "^2.1.1",
    "chroma-js": "^3.1.2",
    "downshift": "^9.0.13",
    "immutable": "^5.0.3",
    "js-base64": "^3.7.7",
    "katex": "^0.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "slate": "^0.112.0",
    "slate-dom": "^0.112.2",
    "slate-history": "^0.110.3",
    "slate-react": "^0.112.1",
    "wouter": "^3.6.0"
  },
  "resolutions": {
    "@types/slate*/**/immutable": "5.0.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/chroma-js": "^2.4.4",
    "@types/jest": "^29.5.14",
    "@types/katex": "^0.11.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "needs-format": "prettier -l '**/*.ts' '**/*.tsx'",
    "format": "prettier --write '**/*.ts' '**/*.tsx'",
    "lint": "eslint .",
    "test": "jest",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf ./lib ./lib.browser",
    "build": "tsc && tsc -p tsconfig.browser.json && yarn build:css-stubs && yarn build:copy-css",
    "build:css-stubs": "find . -name '*.module.css' -not -path './lib*' -not -path './node_modules/*' | while read src; do dir=$(dirname \"$src\" | sed 's|^\\./||'); mkdir -p \"lib/$dir\"; echo \"module.exports = new Proxy({}, { get: () => '' });\" > \"lib/$dir/$(basename \"$src\")\"; done",
    "build:copy-css": "find . -name '*.css' -not -path './lib*' -not -path './node_modules/*' | while read src; do dir=$(dirname \"$src\" | sed 's|^\\./||'); mkdir -p \"lib.browser/$dir\"; cp \"$src\" \"lib.browser/$dir/\"; done"
  }
}
