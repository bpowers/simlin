{
  "name": "@system-dynamics/diagram",
  "version": "1.3.0",
  "description": "System Dynamics stock and flow diagram display (and editing)",
  "repository": "http://github.com/bpowers/model-app",
  "author": "Bobby Powers <bobbypowers@gmail.com>",
  "license": "Apache-2.0",
  "sideEffects": false,
  "browser": "lib.browser",
  "main": "lib",
  "exports": {
    "./*": {
      "browser": "./lib.browser/*.js",
      "node": "./lib/*.js"
    },
    ".": {
      "browser": "./lib.browser/index.js",
      "node": "./lib/index.js"
    }
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0",
    "@system-dynamics/core": "^1.3.4",
    "@system-dynamics/engine": "^1.3.0",
    "@system-dynamics/importer": "^1.3.5",
    "chroma-js": "^2.1.0",
    "google-protobuf": "^3.18.0",
    "immutable": "^4.0.0",
    "js-base64": "^3.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "slate": "^0.66.1",
    "slate-history": "^0.66.0",
    "slate-react": "^0.66.1"
  },
  "resolutions": {
    "@types/slate*/**/immutable": "4.0.0",
    "immer": "9.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@mui/icons-material": "^5.0.0",
    "@types/chroma-js": "^2.1.3",
    "@types/katex": "^0.11.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^4.0.0",
    "katex": "^0.13.18",
    "prettier": "^2.0.1",
    "recharts": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "needs-format": "prettier -l '**/*.ts' '**/*.tsx'",
    "format": "prettier --write '**/*.ts' '**/*.tsx'",
    "lint": "eslint '**/*.ts' '**/*.tsx'",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf ./lib ./lib.browser",
    "build": "tsc && tsc -p tsconfig.browser.json"
  }
}
