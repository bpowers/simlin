        {
            "name": "main",
            "stocks": [
                {
                    "name": "hero_culture",
                    "initialEquation": "10",
                    "units": "dimensionless",
                    "inflows": [
                        "culture_reinforcement"
                    ],
                    "outflows": [
                        "culture_decay"
                    ],
                    "documentation": "Organizational preference for and reliance on heroic firefighting (0-100 scale)"
                },
                {
                    "name": "incidents",
                    "initialEquation": "5",
                    "units": "incidents",
                    "inflows": [
                        "incident_occurrence"
                    ],
                    "outflows": [
                        "incident_resolution"
                    ],
                    "documentation": "Active incidents requiring resolution"
                },
                {
                    "name": "system_reliability",
                    "initialEquation": "80",
                    "units": "dimensionless",
                    "inflows": [
                        "reliability_improvement"
                    ],
                    "outflows": [
                        "reliability_erosion"
                    ],
                    "documentation": "Overall health and stability of the software systems (0-100 scale)"
                },
                {
                    "name": "technical_debt",
                    "initialEquation": "20",
                    "units": "dimensionless",
                    "inflows": [
                        "debt_accumulation"
                    ],
                    "outflows": [
                        "debt_paydown"
                    ],
                    "documentation": "Accumulated shortcuts, workarounds, and deferred maintenance (0-100 scale)"
                }
            ],
            "flows": [
                {
                    "name": "culture_decay",
                    "equation": "Hero_Culture * culture_decay_fraction",
                    "units": "dimensionless/Month",
                    "documentation": "Natural decay of hero culture without reinforcement"
                },
                {
                    "name": "culture_reinforcement",
                    "equation": "Incidents * heroic_response_fraction * hero_recognition_factor / response_time",
                    "units": "dimensionless/Month",
                    "documentation": "Recognition and rewards for firefighting reinforce hero culture"
                },
                {
                    "name": "debt_accumulation",
                    "equation": "Incidents * heroic_response_fraction * debt_per_heroic_fix / response_time",
                    "units": "dimensionless/Month",
                    "documentation": "Technical debt from heroic quick fixes that skip proper solutions"
                },
                {
                    "name": "debt_paydown",
                    "equation": "investment_in_prevention * debt_paydown_effectiveness",
                    "units": "dimensionless/Month",
                    "documentation": "Technical debt reduction through proper engineering investment"
                },
                {
                    "name": "incident_occurrence",
                    "equation": "base_incident_rate * (100 - System_Reliability) / 50 + Technical_Debt * debt_incident_factor",
                    "units": "incidents/Month",
                    "documentation": "New incidents arising from system fragility and technical debt"
                },
                {
                    "name": "incident_resolution",
                    "equation": "Incidents * (heroic_response_fraction + fundamental_fix_fraction) / response_time",
                    "units": "incidents/Month",
                    "documentation": "Incidents resolved through heroic intervention or fundamental fixes"
                },
                {
                    "name": "reliability_erosion",
                    "equation": "Incidents * heroic_response_fraction * quick_fix_erosion / response_time + Technical_Debt * debt_erosion_factor",
                    "units": "dimensionless/Month",
                    "documentation": "Reliability loss from quick fixes and mounting tech debt"
                },
                {
                    "name": "reliability_improvement",
                    "equation": "investment_in_prevention * prevention_effectiveness",
                    "units": "dimensionless/Month",
                    "documentation": "Improvements from proactive reliability investments"
                }
            ],
            "auxiliaries": [
                {
                    "name": "base_incident_rate",
                    "equation": "2",
                    "units": "incidents/Month",
                    "documentation": "Baseline rate of incidents in a normal system"
                },
                {
                    "name": "culture_decay_fraction",
                    "equation": "0.02",
                    "units": "1/Month",
                    "documentation": "Natural decay rate of hero culture"
                },
                {
                    "name": "debt_erosion_factor",
                    "equation": "0.02",
                    "units": "1/Month",
                    "documentation": "Rate at which technical debt erodes reliability"
                },
                {
                    "name": "debt_incident_factor",
                    "equation": "0.05",
                    "units": "incidents/Month",
                    "documentation": "Incidents generated per unit of technical debt"
                },
                {
                    "name": "debt_paydown_effectiveness",
                    "equation": "0.01",
                    "units": "dimensionless",
                    "documentation": "Rate at which prevention investment pays down debt"
                },
                {
                    "name": "debt_per_heroic_fix",
                    "equation": "0.3",
                    "units": "dimensionless/incidents",
                    "documentation": "Technical debt created per incident fixed heroically"
                },
                {
                    "name": "fundamental_fix_fraction",
                    "equation": "0.2 * (1 - Hero_Culture / 100)",
                    "units": "dimensionless",
                    "documentation": "Fraction of incidents fixed properly, decreases as hero culture dominates"
                },
                {
                    "name": "hero_recognition_factor",
                    "equation": "0.05",
                    "units": "dimensionless/incidents",
                    "documentation": "How much hero culture grows per heroic incident resolution"
                },
                {
                    "name": "heroic_response_fraction",
                    "equation": "0.3 * (1 + Hero_Culture / 50)",
                    "units": "dimensionless",
                    "documentation": "Fraction of incidents addressed heroically, increases with hero culture"
                },
                {
                    "name": "investment_in_prevention",
                    "equation": "total_engineering_capacity * (1 - Hero_Culture / 100) * prevention_priority",
                    "units": "dimensionless/Month",
                    "documentation": "Resources devoted to proactive reliability work"
                },
                {
                    "name": "prevention_effectiveness",
                    "equation": "0.02",
                    "units": "dimensionless",
                    "documentation": "How much reliability improves per unit of prevention investment"
                },
                {
                    "name": "prevention_priority",
                    "equation": "0.003",
                    "units": "1/Month",
                    "documentation": "Management priority given to prevention vs. feature work"
                },
                {
                    "name": "quick_fix_erosion",
                    "equation": "0.1",
                    "units": "dimensionless/incidents",
                    "documentation": "Reliability lost per incident resolved via quick fix"
                },
                {
                    "name": "response_time",
                    "equation": "2",
                    "units": "Month",
                    "documentation": "Average time to resolve an incident"
                },
                {
                    "name": "total_engineering_capacity",
                    "equation": "100",
                    "units": "dimensionless",
                    "documentation": "Total available engineering capacity (normalized)"
                }
            ],
