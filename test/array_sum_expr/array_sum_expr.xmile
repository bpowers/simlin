<?xml version="1.0" encoding="utf-8"?>
<xmile version="1.0" xmlns="http://docs.oasis-open.org/xmile/ns/XMILE/v1.0">
    <header>
        <vendor>Test</vendor>
        <product>Simlin</product>
    </header>
    <sim_specs method="Euler" time_units="Time">
        <start>0</start>
        <stop>2</stop>
        <dt>1</dt>
    </sim_specs>
    <dimensions>
        <dim name="Items">
            <elem name="A"/>
            <elem name="B"/>
            <elem name="C"/>
        </dim>
    </dimensions>
    <model>
        <variables>
            <aux name="values">
                <element subscript="A">
                    <eqn>1</eqn>
                </element>
                <element subscript="B">
                    <eqn>2</eqn>
                </element>
                <element subscript="C">
                    <eqn>3</eqn>
                </element>
                <dimensions>
                    <dim name="Items"/>
                </dimensions>
            </aux>
            <aux name="sum_plus_one">
                <doc>SUM(values[*] + 1) = (1+1) + (2+1) + (3+1) = 9</doc>
                <eqn>SUM(values[*] + 1)</eqn>
            </aux>
            <aux name="sum_times_two">
                <doc>SUM(values[*] * 2) = (1*2) + (2*2) + (3*2) = 12</doc>
                <eqn>SUM(values[*] * 2)</eqn>
            </aux>
        </variables>
    </model>
</xmile>
