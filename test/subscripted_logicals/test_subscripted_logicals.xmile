<?xml version="1.0" encoding="utf-8"?>
<xmile version="1.0" xmlns="http://docs.oasis-open.org/xmile/ns/XMILE/v1.0" xmlns:isee="http://iseesystems.com/XMILE" xmlns:simlin="https://simlin.com/XMILE/v1.0">
    <header>
        <vendor>Simlin</vendor>
        <product version="0.1.0" lang="en">Simlin</product>
    </header>
    <sim_specs method="euler" time_units="Month">
        <start>0</start>
        <stop>10</stop>
        <dt>1</dt>
    </sim_specs>
    <dimensions>
        <dim name="dim1">
            <elem name="a"></elem>
            <elem name="b"></elem>
            <elem name="c"></elem>
            <elem name="d"></elem>
        </dim>
        <dim name="dim2">
            <elem name="e"></elem>
            <elem name="f"></elem>
            <elem name="g"></elem>
            <elem name="h"></elem>
        </dim>
    </dimensions>
    <model>
        <variables>
            <aux name="scalar_and_scalar">
                <eqn>( IF Time &gt; 5 and Time &lt; 8 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="scalar_and_array">
                <dimensions>
                    <dim name="dim1"></dim>
                </dimensions>
                <eqn>( IF Time &gt; 5 and array_1d[dim1]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="not_array">
                <dimensions>
                    <dim name="dim1"></dim>
                </dimensions>
                <eqn>( IF  not array_1d[dim1]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array1d_or_array2d">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF array_1d[dim1]*Time &gt; 5 or array_2d[dim1, dim2]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array1d_and_array2d">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF array_1d[dim1]*Time &gt; 5 and array_2d[dim1, dim2]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array1d_and_array1db">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF array_1d[dim1]*Time &gt; 5 and array_1db[dim2]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array_2d_or_array_1db">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF array_2d[dim1, dim2]*Time &gt; 5 or array_1db[dim2]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array_1db">
                <dimensions>
                    <dim name="dim2"></dim>
                </dimensions>
                <element subscript="e">
                    <eqn>0</eqn>
                </element>
                <element subscript="f">
                    <eqn>10</eqn>
                </element>
                <element subscript="g">
                    <eqn>20</eqn>
                </element>
                <element subscript="h">
                    <eqn>1</eqn>
                </element>
            </aux>
            <aux name="array_1d_or_array_1d">
                <dimensions>
                    <dim name="dim1"></dim>
                </dimensions>
                <eqn>( IF array_1d[dim1]*Time &gt; 5 or array_1d[dim1]*Time &lt; 10 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="scalar_or_array">
                <dimensions>
                    <dim name="dim1"></dim>
                </dimensions>
                <eqn>( IF Time &gt; 5 or array_1d[dim1]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array_2d_and_array_1db">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF array_2d[dim1, dim2]*Time &gt; 5 and array_1db[dim2]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array_1d_and_array_1d">
                <dimensions>
                    <dim name="dim1"></dim>
                </dimensions>
                <eqn>( IF array_1d[dim1]*Time &gt; 5 and array_1d[dim1]*Time &lt; 10 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array1d_or_array1db">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF array_1d[dim1]*Time &gt; 5 or array_1db[dim2]*Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="not_and_or">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <eqn>( IF ( not Time &gt; 5 or array_1d[dim1]*Time &gt; 5) and ( not array_2d[dim1, dim2]*Time &gt; 5 and array_1db[dim2] &gt; 5) THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="scalar_or_scalar">
                <eqn>( IF Time &lt; 5 or Time &gt; 8 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array_2d">
                <dimensions>
                    <dim name="dim1"></dim>
                    <dim name="dim2"></dim>
                </dimensions>
                <element subscript="a,e">
                    <eqn>2</eqn>
                </element>
                <element subscript="a,f">
                    <eqn>6</eqn>
                </element>
                <element subscript="a,g">
                    <eqn>1</eqn>
                </element>
                <element subscript="a,h">
                    <eqn>0</eqn>
                </element>
                <element subscript="b,e">
                    <eqn>0</eqn>
                </element>
                <element subscript="b,f">
                    <eqn>0</eqn>
                </element>
                <element subscript="b,g">
                    <eqn>10</eqn>
                </element>
                <element subscript="b,h">
                    <eqn>5</eqn>
                </element>
                <element subscript="c,e">
                    <eqn>4</eqn>
                </element>
                <element subscript="c,f">
                    <eqn>2</eqn>
                </element>
                <element subscript="c,g">
                    <eqn>1</eqn>
                </element>
                <element subscript="c,h">
                    <eqn>7</eqn>
                </element>
                <element subscript="d,e">
                    <eqn>2</eqn>
                </element>
                <element subscript="d,f">
                    <eqn>8</eqn>
                </element>
                <element subscript="d,g">
                    <eqn>2</eqn>
                </element>
                <element subscript="d,h">
                    <eqn>6</eqn>
                </element>
            </aux>
            <aux name="not_scalar">
                <eqn>( IF  not Time &gt; 5 THEN 0 ELSE 1 )</eqn>
            </aux>
            <aux name="array_1d">
                <dimensions>
                    <dim name="dim1"></dim>
                </dimensions>
                <element subscript="a">
                    <eqn>1</eqn>
                </element>
                <element subscript="b">
                    <eqn>0</eqn>
                </element>
                <element subscript="c">
                    <eqn>2</eqn>
                </element>
                <element subscript="d">
                    <eqn>10</eqn>
                </element>
            </aux>
        </variables>
        <views>
            <view isee:show_pages="false" page_width="800" page_height="600" view_type="stock_flow">
                <aux name="array_1d" x="246" y="200" label_side="bottom"></aux>
                <aux name="array_2d" x="248" y="332" label_side="bottom"></aux>
