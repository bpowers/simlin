                </connector>
                <connector angle="-33.9553783505354">
                    <from>array_1db</from>
                    <to>array_2d_or_array_1db</to>
                </connector>
                <connector angle="5.790888529548283">
                    <from>array_2d</from>
                    <to>array1d_or_array2d</to>
                </connector>
                <aux name="array_2d_and_array_1db" x="112" y="377" label_side="bottom"></aux>
                <aux name="array1d_and_array2d" x="100" y="306" label_side="bottom"></aux>
                <aux name="array1d_and_array1db" x="106" y="251" label_side="bottom"></aux>
                <aux name="array_1d_and_array_1d" x="111" y="205" label_side="bottom"></aux>
                <aux name="scalar_and_array" x="115" y="165" label_side="bottom"></aux>
                <aux name="scalar_and_scalar" x="119" y="132" label_side="bottom"></aux>
                <connector angle="165.31308984071387">
